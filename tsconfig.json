{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "./out-tsc/app",
    "target": "es5",
    "module": "es2015",
    "baseUrl": "./src",
    "paths": {
      "@fs/*": ["app/*"],
      "@env/*": ["environments/*"]
    },
    "typeRoots": [
      "./node_modules/@types"
    ]
  }
}
